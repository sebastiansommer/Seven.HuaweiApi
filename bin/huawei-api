#!/usr/bin/env php
<?php
require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Seven\HuaweiApi\Command\IpCommand;
use Seven\HuaweiApi\Command\ModemCommand;

$application = new Application('Seven.HuaweiApi', '0.1-dev');
$application->add(new IpCommand());
$application->add(new ModemCommand());
$application->run();